{
  "name": "jbboehr/dh-make-composer",
  "description": "Command-line utility that builds a package from composer project.",
  "keywords": [
    "deb",
    "packages",
    "software distribution"
  ],
  "license": "LGPL-2.1",
  "autoload": {
    "psr-4": {
      "DhMakeComposer\\": "src"
    }
  },
  "require": {
    "symfony/console": "2.6.7",
    "symfony/filesystem": "2.6.7",
    "symfony/process": "2.6.7"
  },
  "require-dev": {
    "phpunit/phpunit": "4.6"
  },
  "bin": [
    "bin/dh_make_composer"
  ]
}