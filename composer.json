{
    "name": "jbboehr/dh-make-composer",
    "description": "Command-line utility that builds a debian package from composer package.",
    "keywords": [
        "deb",
        "packages",
        "software distribution"
    ],
    "license": "LGPL-2.1",
    "autoload": {
        "psr-4": {
            "DhMakeComposer\\": "src"
        }
    },
    "require": {
        "php": "~7.0",
        "composer/composer": "~1.0.0",
        "symfony/console": "~2.7",
        "symfony/filesystem": "~2.7",
        "symfony/process": "~2.7"
    },
    "suggest": {
        "ext-zip": "*"
    },
    "bin": [
        "bin/dh_make_composer"
    ]
}
